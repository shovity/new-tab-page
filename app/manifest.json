{
  "name": "New Tab Page",
  "description": "Bootstrap tab and new tab",
  "version": "1.0",

 "chrome_url_overrides" : {
    "newtab": "newtab.html"
  },

  "background": {
    "scripts": ["background.bundle.js"]
